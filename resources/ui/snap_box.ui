<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="SnapBoxWidget" parent="GtkBox">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="spacing">1</property>
    <property name="homogeneous">False</property>
    <child>
      <object class="GtkToggleButton" id="snap_to_grid">
        <property name="focusable">1</property>
        <property name="receives_default">1</property>
        <property name="action_name">app.snap-to-grid</property>
        <property name="action_target">&quot;timeline&quot;</property>
        <property name="tooltip-text" translatable="yes">Snap to grid</property>
        <child>
          <object class="GtkImage">
            <property name="icon-name">gnome-builder-ui-packing-symbolic-light</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkToggleButton" id="snap_to_grid_keep_offset">
        <property name="focusable">1</property>
        <property name="receives_default">1</property>
        <property name="action_name">app.snap-keep-offset</property>
        <property name="action_target">&quot;timeline&quot;</property>
        <property name="tooltip-text" translatable="yes">Keep offset</property>
        <child>
          <object class="GtkImage">
            <property name="icon-name">gnome-builder-widget-layout-symbolic-light</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkToggleButton" id="snap_to_events">
        <property name="focusable">1</property>
        <property name="receives_default">1</property>
        <property name="action_name">app.snap-events</property>
        <property name="action_target">&quot;timeline&quot;</property>
        <property name="tooltip-text" translatable="yes">Snap to events</property>
        <child>
          <object class="GtkImage">
            <property name="icon-name">gnome-builder-builder-split-tab-right-symbolic-light</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="SnapGridWidget" id="snap_grid">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="receives_default">True</property>
      </object>
    </child>
  </template>
</interface>
